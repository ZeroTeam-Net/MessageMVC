<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <ApplicationIcon />
    <StartupObject />
    <Description>ZeroTeam.MessageMVC抽象定义
以MVC方式进行消息处理,支持常用消息队列以及其它RPC方式的消息处理</Description>
    <Version>1.0.0.148</Version>
    <PackageLicenseExpression>MPL-2.0</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/ZeroTeam-Net/MessageMVC</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ZeroTeam-Net/MessageMVC.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>ZeroTeam.MessageMVC.Abstractions.xml</DocumentationFile>
    <DefineConstants>TRACE;LogMonitor</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>ZeroTeam.MessageMVC.Abstractions.xml</DocumentationFile>
    <DefineConstants>TRACE;LogMonitor</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Auth\**" />
    <Compile Remove="WebApi\**" />
    <EmbeddedResource Remove="Auth\**" />
    <EmbeddedResource Remove="WebApi\**" />
    <None Remove="Auth\**" />
    <None Remove="WebApi\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="API\ActionArgumentConvert.cs" />
    <Compile Remove="API\MessageArgumentConvert.cs" />
    <Compile Remove="Config\SpeedLimitType.cs" />
    <Compile Remove="Config\UserOperatorStateType.cs" />
    <Compile Remove="Config\ZeroOperatorStateType.cs" />
    <Compile Remove="Context\ArgumentType.cs" />
    <Compile Remove="Context\ContextOption.cs" />
    <Compile Remove="Context\RequestType.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="._.vs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Agebull.Common.LogRecorder" Version="4.0.0.30" />
    <PackageReference Include="System.ComponentModel.Composition" Version="4.7.0" />
  </ItemGroup>

</Project>
