{
  "ASPNETCORE_ENVIRONMENT_": "Development",
  "ZeroApp": {
    ///线程池最大工作线程数
    "MaxWorkThreads": 0,
    ///   线程池最大IO线程数
    "MaxIOThreads": 0,
    ///     数据/日志/其它配置使用AppName为文件夹
    "IsolateFolder": false,
    ///     短名称
    "ShortName": null,
    ///     本地数据文件夹
    "DataFolder": null,
    ///     启用Monitor模式日志记录
    "EnableMonitorLog": true,
    ///     本地配置文件夹
    "ConfigFolder": null,
    ///     启用插件自动加载
    "EnableAddIn": false,
    ///     插件地址,如为空则与运行目录相同
    "AddInPath": null,
    ///     启用调用链跟踪(使用IZeroContext全局上下文)
    "EnableLinkTrace": false,
    ///     启用异常消息本地重放
    "EnableMessageReConsumer": false,
    ///     启用埋点中间件
    "EnableMarkPoint": false,
    ///     埋点服务名称
    "MarkPointName": "MarkPoint"
  },
  "Redis": {
    "ConnectionString": "47.98.229.139",
    "GuardCheckTime": 3000,
    "MessageLockTime": 3000,
    "FailedIsError": false,
    "NoSupperIsError": false
  },
  "MessagePoster": {
    "default": "CsRedisProducer"
  },
  "WebSocket": {
    "Folders": ["MarkPoint"]
  },
  "Logging": {
    /*不注册日志器*/
    "noRegist": true,
    /*是否开启标准控制台日志,动态修改无效*/
    "console": true,
    /*是否使用基础文本日志记录器,动态修改无效*/
    "innerLogger": false,
    /*是否开启跟踪日志,可动态修改*/
    "monitor": true,
    /*是否开启SQL语句日志,可动态修改*/
    "sql": false,
    /*基础文本日志记录器配置*/
    "Text": {
      /*日志路径,动态修改可能延迟生效,如未设置,则会被设置为与应用数据文件夹同级的logs目录
      "path": "e:\\projects\\logs",*/
      /*是否按日期分文件夹,动态修改可能延迟生效*/
      "dayFolder": false,
      /*最大文件数量,可动态修改*/
      "disable": true,
      /*最大文件数量(dayFolder为false有效),可动态修改*/
      "maxFile": 99,
      "LogLevel": "Information"
    },
    /*标准日志设置,参考NetCore相关内容*/
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Information"
    },
    "Console": {
      "Format": "Systemd"
    }
  },
  "Kestrel": {
    "AddServerHeader": true,
    "AllowSynchronousIO": false,
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    },
    "Limits": {
      "MinResponseDataRate": null
    },
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5050"
      }
    }
  }
}