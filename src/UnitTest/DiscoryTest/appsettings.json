{
  "ASPNETCORE_ENVIRONMENT_": "Development",
  "MessageMVC": {
    "Option": {
      ,
      ///线程池最大工作线程数
      "MaxWorkThreads": 0,
      ///   线程池最大IO线程数
      "MaxIOThreads": 0,
      //数据/日志/其它配置使用AppName为文件夹
      "IsolateFolder": false,
      //短名称
      "ShortName": null,
      //本地数据文件夹
      "DataFolder": null,
      //本地配置文件夹
      "ConfigFolder": null,
      //启用插件自动加载
      "EnableAddIn": true,
      //插件地址,如为空则与运行目录相同
      "AddInPath": null
    },
    "Tools": {
      //启用Monitor模式日志记录
      "EnableMonitorLog": true,
      //启用调用链跟踪(使用IZeroContext全局上下文)
      "EnableLinkTrace": true,
      //启用异常消息本地重放
      "EnableMessageReConsumer": true,
      //启用埋点中间件
      "EnableMarkPoint": true,
      //回执服务名称
      "MarkPointName": "MarkPoint",
      //回执接口方法
      "ReceiptService": "TrdReceipt",
      //回执接口方法
      "ReceiptApi": "receipt/v1/save"
    },
    "Redis": {
      "ConnectionString": "192.168.240.132",
      "GuardCheckTime": 3000,
      "MessageLockTime": 3000,
      "FailedIsError": false,
      "NoSupperIsError": false
    },
    "PlanPost": {
      "ServiceName": "PlanTask",
      "PostApiName": "v1/post"
    },
    "HttpClient": {
      "DefaultUrl": "http://www.baidu.com",
      "DefaultTimeOut": 3,
      "Clients": []
    },
    "MessagePoster": {
      "default": "HttpPoster",
      "KafkaPoster": "MarkPoint"
    },
    "Kafka": {
      "BootstrapServers": "192.168.240.132:9092",
      "Producer": {
        "ApiVersionFallbackMs": 100
      },
      "Client": {
        "ApiVersionFallbackMs": 100
      },
      "Consumer": {
        "GroupId": "kaisen",
        "AutoOffsetReset": "Earliest",
        "EnableAutoCommit": false
      }
    },
    "HttpRoute": {
      /// 启用文件上传
      "EnableFormFile": false,
      /// 启用身份令牌
      "EnableAuthToken": false,
      /// 启用Header跟踪(如HTTP请求头)
      "EnableHeader": false,
      /// 特殊URL取第几个路径作为服务名称的映射表
      "HostPaths": false,
      ///启用快速调用,即直接使用ApiExecuter
      "FastCall": false
    }
  },
  "Logging": {
    /*不注册日志器*/
    "noRegist": true,
    /*是否启动标准控制台日志,动态修改无效*/
    "console": true,
    /*是否使用基础文本日志记录器,动态修改无效*/
    "innerLogger": true,
    /*是否启动跟踪日志,可动态修改*/
    "monitor": true,
    /*是否启动SQL语句日志,可动态修改*/
    "sql": true,
    /*基础文本日志记录器配置*/
    "Text": {
      /*日志路径,动态修改可能延迟生效,如未设置,则会被设置为与应用数据文件夹同级的logs目录*/
      "path": null,
      /*是否按日期分文件夹,动态修改可能延迟生效*/
      "dayFolder": false,
      /*最大文件数量,可动态修改*/
      "disable": true,
      /*最大文件数量(dayFolder为false有效),可动态修改*/
      "maxFile": 99,
      "LogLevel": "Information"
    },
    /*标准日志设置,参考NetCore相关内容*/
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Error",
      "ZeroFlowControl": "Error"
    },
    "Console": {
      //"Format": "Systemd"
    }
  }
}