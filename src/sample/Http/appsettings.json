{
  "ASPNETCORE_ENVIRONMENT_": "Development",
  "MessageMVC": {
    "Option": {
      "IsOpenAccess": true,
      "UseZero": true,
      ///线程池最大工作线程数
      "MaxWorkThreads": 0,
      ///   线程池最大IO线程数
      "MaxIOThreads": 0,
      //数据/日志/其它配置使用AppName为文件夹
      "IsolateFolder": false,
      //短名称
      "AppName": "HttpSample",
      //短名称
      "ShortName": "HttpSample",
      //本地数据文件夹
      "DataFolder": null,
      //本地配置文件夹
      "ConfigFolder": null,
      //启用插件自动加载
      "EnableAddIn": true,
      //插件地址,如为空则与运行目录相同
      "AddInPath": null,
      "ServiceMap": {
        "PlanTest": "abc"
      }
    },
    "Tools": {
      //启用调用链跟踪(使用IZeroContext全局上下文)
      "EnableLinkTrace": false,
      //启用异常消息本地重放
      "EnableMessageReConsumer": false,
      //启用埋点中间件
      "EnableMarkPoint": true,
      //回执服务名称
      "MarkPointName": "MarkPoint",
      //回执接口方法
      "ReceiptService": "TrdReceipt",
      //回执接口方法
      "ReceiptApi": "receipt/v1/save"
    },
    "ApiContract": {
      //启用返回值跟踪
      "EnableResultTrace": false,
      //返回值跟踪包含机器名
      "TraceMachine": true
    },
    "Redis": {
      "ConnectionString": "192.168.240.132",
      "GuardCheckTime": 3000,
      "MessageLockTime": 3000,
      "FailedIsError": false,
      "NoSupperIsError": false
    },
    "PlanService": {
      "ServiceName": "PlanTask",
      "PostApiName": "v1/post"
    },
    "Kafka": {
      "BootstrapServers": "192.168.240.132:9092",
      "GroupId": "kaisen",
      "AutoOffsetReset": "Earliest",
      "EnableAutoCommit": false
    },
    "ConfigSync": {
      "ConnectionString": "192.168.240.132"
    },
    "MessagePoster": {
      //启用本地隧道（即本地接收器存在的话，本地处理）
      "localTunnel": true,
      "default": "HttpPoster",
      "HttpPoster": "ConfigEdit",
      "CsRedisPoster": "PlanTask",
      "KafkaPoster": "MarkPoint"
    },
    "ParallelService": {
      "ParallelTest": "HttpTest1,HttpTest2"
    }
  },
  "Logging": {
    "LogRecorder": {
      /*不注册日志器*/
      "noRegist": true,
      /*是否启动跟踪日志,可动态修改*/
      "monitor": true,
      /*跟踪日志是否包含详细信息,可动态修改*/
      "details": true,
      /*是否启动SQL语句日志,可动态修改*/
      "sql": false
    },
    /*标准日志设置,参考NetCore相关内容*/
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Information"
    },
    "Console": {
      //"Format": "Systemd"
    }
  },
  "Http": {
    "Url": "http://0.0.0.0:8088",
    "Client": {
      "DefaultUrl": "http://localhost:8000",
      "DefaultTimeOut": 30,
      "Services": [
        {
          "Name": "ConfigEdit",
          "Alias": "ce",
          "Url": "http://192.168.240.132:8002"
        }
      ]
    },
    "Message": {
      /// 启用文件上传
      "EnableFormFile": false,
      /// 启用身份令牌
      "EnableAuthToken": false,
      /// 启用Header跟踪(如HTTP请求头)
      "EnableHeader": false,
      /// 特殊URL取第几个路径作为服务名称的映射表
      "HostPaths": 0,
      ///启用快速调用,即直接使用ApiExecuter
      "FastCall": false
    },
    "Kestrel": {
      "AddServerHeader": true,
      "AllowSynchronousIO": false,
      "EndpointDefaults": {
        "Protocols": "Http1AndHttp2"
      },
      "Limits": {
        "MinResponseDataRate": null
      }
      //,"Endpoints": {
      //  "Http": {
      //    "Url": "http://0.0.0.0:8080"
      //  }
      //  ,
      //  "Https": {
      //    "Url": "https://*:5004",
      //    "Certificate": {
      //      "Path": "<path to .pfx file>",
      //      "Password": "<certificate password>"
      //    }
      //  }
      //}
    }
  }
}