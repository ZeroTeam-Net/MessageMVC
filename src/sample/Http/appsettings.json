{
  "ASPNETCORE_ENVIRONMENT_": "Development",
  "MessageMVC": {
    "Option": {
      "IsOpenAccess": true,
      //使用上级目录作为基础目录
      "IsolateFolder": true,
      ///线程池最大工作线程数
      "MaxWorkThreads": 0,
      ///   线程池最大IO线程数
      "MaxIOThreads": 0,
      //使用上级目录作为基础目录
      "ZeroFolder": true,
      //短名称
      "AppName": "HttpSample",
      //短名称
      "ShortName": "HttpSample",
      //本地数据文件夹
      "DataFolder": null,
      //本地配置文件夹
      "ConfigFolder": null,
      //启用插件自动加载
      "EnableAddIn": true,
      //插件地址,如为空则与运行目录相同
      "AddInPath": null
    },

    "ServiceMap": {
      "PlanTest": "abc"
    },
    "Tools": {
      //启用调用链跟踪(使用IZeroContext全局上下文)
      "EnableLinkTrace": false,
      //启用异常消息本地重放
      "EnableMessageReConsumer": false,
      //启用埋点中间件
      "EnableMarkPoint": false,
      //回执服务名称
      "MarkPointName": "MarkPoint",
      //回执接口方法
      "ReceiptService": "TrdReceipt",
      //回执接口方法
      "ReceiptApi": "receipt/v1/save"
    },
    "ApiContract": {
      //启用返回值跟踪
      "EnableResultTrace": false,
      //返回值跟踪包含机器名
      "TraceMachine": true
    },
    "Redis": {
      "ConnectionString": "118.25.127.80,password=sk9Y^q%^#RiCiBt&",
      "GuardCheckTime": 3000,
      "MessageLockTime": 3000,
      "FailedIsError": false,
      "NoSupperIsError": false
    },
    "PlanService": {
      "ServiceName": "PlanTask",
      "PostApiName": "v1/post"
    },
    "Kafka": {
      "BootstrapServers": "118.25.127.80:9092",
      "Producer": {
        "ApiVersionFallbackMs": 100
      },
      "Client": {
        "ApiVersionFallbackMs": 100
      },
      "Consumer": {
        "GroupId": "Agebull",
        "AutoOffsetReset": "Earliest",
        "EnableAutoCommit": false
      }
    },
    "ConfigSync": {
      "ConnectionString": "192.168.240.132"
    },
    "MessagePoster": {
