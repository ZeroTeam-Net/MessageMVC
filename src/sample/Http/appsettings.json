      "HttpPoster": "ConfigEdit",
      "CsRedisPoster": "PlanTask",
      "KafkaPoster": "MarkPoint",
      "TcpPoster": "log"
    },
    "ParallelService": {
      "ParallelTest": "HttpTest1,HttpTest2"
    },
    "Tcp": {
      "Client": {
        "Port": 8800,
        "Address": "118.25.127.80",
        "IsLog": true,
        "Level":"Trace"
      }
    }
  },
  "Logging": {
    "Console": true,
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Debug",
      "MessageMVC": "Debug"
    }
  },
  "Http": {
    "Url": "http://0.0.0.0:8088",
    "Client": {
      "DefaultUrl": "http://localhost:8080",
      "DefaultTimeOut": 30
    },
    "Message": {
      /// 启用文件上传
      "EnableFormFile": false,
      /// 启用身份令牌
      "EnableAuthToken": false,
      /// 启用Header跟踪(如HTTP请求头)
      "EnableHeader": false,
      /// 特殊URL取第几个路径作为服务名称的映射表
      "HostPaths": 0,
      ///启用快速调用,即直接使用ApiExecuter
      "FastCall": false
    },
    "Kestrel": {
      "AddServerHeader": true,
      "AllowSynchronousIO": false,
      "EndpointDefaults": {
        "Protocols": "Http1AndHttp2"
      },
      "Limits": {
        "MinResponseDataRate": null
      },
      "Endpoints": {
        "Http": {
          "Url": "http://0.0.0.0:8080"
        }
