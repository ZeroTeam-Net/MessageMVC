{
  "ASPNETCORE_ENVIRONMENT_": "Development",
  "MessageMVC": {
    "Option": {
      "UseZero": true,
      ///线程池最大工作线程数
      "MaxWorkThreads": 0,
      ///   线程池最大IO线程数
      "MaxIOThreads": 0,
      //数据/日志/其它配置使用AppName为文件夹
      "IsolateFolder": false,
      //短名称
      "ShortName": null,
      //本地数据文件夹
      "DataFolder": null,
      //本地配置文件夹
      "ConfigFolder": null,
      //启用插件自动加载
      "EnableAddIn": true,
      //插件地址,如为空则与运行目录相同
      "AddInPath": null
    },
    "Tools": {
      //启用调用链跟踪(使用IZeroContext全局上下文)
      "EnableLinkTrace": false
    },
    "MessagePoster": {
      "default": "CsRedisPoster",
      "HttpPoster": "http,PlanTest"
    },
    "Kafka": {
      "BootstrapServers": "47.103.103.208:9092",
      "GroupId": "age2",
      "AutoOffsetReset": "Earliest",
      "EnableAutoCommit": false
    }
  },
  "Logging": {
    /*不注册日志器*/
    "noRegist": true,
    /*是否开启标准控制台日志,动态修改无效*/
    "console": true,
    /*是否使用基础文本日志记录器,动态修改无效*/
    "innerLogger": false,
    /*是否开启跟踪日志,可动态修改*/
    "monitor": true,
    /*是否开启SQL语句日志,可动态修改*/
    "sql": false,
    /*标准日志设置,参考NetCore相关内容*/
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Information"
    },
    "Console": {
      //"Format": "Systemd"
    }
  }
}