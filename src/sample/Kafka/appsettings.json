{
  "ASPNETCORE_ENVIRONMENT_": "Development",
  "MessageMVC": {
    "Option": {
      "UseZero": true,
      ///线程池最大工作线程数
      "MaxWorkThreads": 1024,
      ///   线程池最大IO线程数
      "MaxIOThreads": 1024,
      //数据/日志/其它配置使用AppName为文件夹
      "IsolateFolder": false,
      //短名称
      "ShortName": null,
      //本地数据文件夹
      "DataFolder": null,
      //本地配置文件夹
      "ConfigFolder": null,
      //启用插件自动加载
      "EnableAddIn": true,
      //插件地址,如为空则与运行目录相同
      "AddInPath": null
    },
    "Tools": {
      //启用调用链跟踪(使用IZeroContext全局上下文)
      "EnableLinkTrace": false
    },
    "MessagePoster": {
      "default": "HttpPoster",
      "KafkaPoster": "Test2"
    },
    "Kafka": {
      "BootstrapServers": "118.25.127.80:9092",
      "Message": {
        "Concurrency": 1024
      },
      "Producer": {
        "ApiVersionFallbackMs": 100
      },
      "Client": {
        "ApiVersionFallbackMs": 100
      },
      "Consumer": {
        "GroupId": "HJ2122017_0_2",
        "AutoOffsetReset": "Earliest"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "MessageMVC": "Information"
    }
  }
}