{
  "ASPNETCORE_ENVIRONMENT_": "Development",
  "AppName": "MulitSample",
  "Redis": {
    "ConnectionString": "192.168.240.132",
    "GuardCheckTime": 3000,
    "MessageLockTime": 3000,
    "FailedIsError": false,
    "NoSupperIsError": false
  },
  "Logging": {
    /*是否启动标准控制台日志,动态修改无效*/
    "console": true,
    /*是否使用基础文本日志记录器,动态修改无效*/
    "innerLogger": false,
    /*是否启动跟踪日志,可动态修改*/
    "monitor": true,
    /*是否启动SQL语句日志,可动态修改*/
    "sql": false,
    /*基础文本日志记录器配置*/
    "Text": {
      /*日志路径,动态修改可能延迟生效,如未设置,则会被设置为与应用数据文件夹同级的logs目录
      "path": "e:\\projects\\logs",*/
      /*是否按日期分文件夹,动态修改可能延迟生效*/
      "dayFolder": false,
      /*最大文件数量,可动态修改*/
      "disable": false,
      /*最大文件数量(dayFolder为false有效),可动态修改*/
      "maxFile": 99,
      "LogLevel": "Information"
    },
    /*标准日志设置,参考NetCore相关内容*/
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
    //,
    //"Console": {
    //  "Format": "Systemd"
    //}
  }
}