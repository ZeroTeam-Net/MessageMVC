{
  "ASPNETCORE_ENVIRONMENT_": "Development",

  "MessageMVC": {
    "Option": {
      "IsOpenAccess": true,
      "UseZero": true,
      ///线程池最大工作线程数
      "MaxWorkThreads": 0,
      ///   线程池最大IO线程数
      "MaxIOThreads": 0,
      //数据/日志/其它配置使用AppName为文件夹
      "IsolateFolder": false,
      //短名称
      "AppName": "HttpSample",
      //短名称
      "ShortName": "HttpSample",
      //本地数据文件夹
      "DataFolder": null,
      //本地配置文件夹
      "ConfigFolder": null,
      //启用插件自动加载
      "EnableAddIn": true,
      //插件地址,如为空则与运行目录相同
      "AddInPath": null
    },
    "Tools": {
      //启用Monitor模式日志记录
      "EnableMonitorLog": true,
      //启用调用链跟踪(启用埋点中间件时自动打开)
      "EnableLinkTrace": false,
      //启用异常消息本地重放
      "EnableMessageReConsumer": true,
      //启用埋点中间件
      "EnableMarkPoint": true,
      //回执服务名称
      "MarkPointName": "MarkPoint",
      //回执接口方法
      "ReceiptService": "TrdReceipt",
      //回执接口方法
      "ReceiptApi": "receipt/v1/save"
    },
    "ApiContract": {
      //启用返回值跟踪
      "EnableResultTrace": true,
      //返回值跟踪包含机器名
      "TraceMachine": false
    },
    "Redis": {
      "ConnectionString": "118.25.127.80,password=sk9Y^q%^#RiCiBt&",
      "GuardCheckTime": 3000,
      "MessageLockTime": 3000,
      "FailedIsError": false,
      "NoSupperIsError": false
    },
    "MessagePoster": {
      "default": "CsRedisEventPoster",
      "CsRedisEventPoster": "EntityEvent,MarkPoint"
    },
    "WebSocket": {
      "Folders": [ "EntityEvent" ]
    }
  },
  "Logging": {
    "FlowTracer": {
      /*不注册日志器*/
      "noRegist": true,
      /*是否使用基础文本日志记录器,动态修改无效*/
      "innerLogger": false,
      /*是否启动跟踪日志,可动态修改*/
      "monitor": true,
      /*跟踪日志是否包含详细信息,可动态修改*/
      "details": false,
      /*是否启动SQL语句日志,可动态修改*/
      "sql": false
    },
    /*标准日志设置,参考NetCore相关内容*/
    "General": {
      "IncludeScopes": false,
      "LogLevel": {
        "Default": "Trace",
        "Microsoft": "Information"
      },
      "Console": {
        //"Format": "Systemd"
      }
    }
  }
}